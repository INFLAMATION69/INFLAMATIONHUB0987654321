local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Inflamation X: Island Gods", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-------------------------------------------------------------

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Drink",
	Callback = function()
local args = {
	[1] = "EatOrDrink",
	[2] = workspace.River
}

game:GetService("ReplicatedStorage").SIGNAL_REMOTE:FireServer(unpack(args))
  	end    
})

Tab:AddButton({
	Name = "Instant Break",
	Callback = function()
while wait() do

local args = {
	[1] = "UseTool",
	[2] = CFrame.new
}

game:GetService("ReplicatedStorage").SIGNAL_REMOTE:FireServer(unpack(args))
end
  	end    
})

Tab:AddButton({
	Name = "Infinite Yield",
	Callback = function()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
  	end    
})

